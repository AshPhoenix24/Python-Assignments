    "        extra=(hours-40)*(rate*1.5)\n",
    "        pay=(40*rate)+extra\n",
    "        print(pay)\n",
    "    elif hours <= 40:\n",
    "        pay=(hours*rate)\n",
    "        print(pay)\n",
    "_compute(hours,rate)       "
